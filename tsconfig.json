{

  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "moduleResolution": "node",
    "declaration": false,
    "noImplicitAny": false,
    "sourceMap": true,
    "jsx": "react",
    "noLib": false,
    "suppressImplicitAnyIndexErrors": true,
    //add for jquery
    "allowSyntheticDefaultImports": true,
    "typeRoots": [
      "node_modules/@types",
      "css"
    ]
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
